{
  "kind": "collectionType",
  "collectionName": "challenges",
  "info": {
    "name": "challenge",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "descriptions": {
      "type": "richtext",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "experience_point": {
      "type": "integer",
      "default": 0,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true
    },
    "users_done": {
      "via": "challenges_done",
      "plugin": "users-permissions",
      "collection": "user",
      "dominant": true
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "name"
    },
    "submissionUrl": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true
    },
    "difficulty": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "easy",
        "medium",
        "hard"
      ],
      "required": true
    }
  }
}
